<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="1080.0" prefWidth="1920.0" stylesheets="@style.css" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.HomeController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox prefHeight="710.0" prefWidth="776.0">
         <children>
            <Button fx:id="addLessonBtn" mnemonicParsing="false" onAction="#onAddLessonBtnPress" text="Add New Lesson">
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="addStudentBtn" mnemonicParsing="false" onAction="#onAddStudentBtnPress" text="Add New Student">
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin></Button>
            <Button fx:id="viewLessonsBth" mnemonicParsing="false" onAction="#onViewLessonBtnPress" text="View Lessons">
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</GridPane>
